{
  "name": "textile-flashcards",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand",
    "test:unit:watch": "vue-cli-service test:unit --watch",
    "textile:check": "npx ncu '/^@textile/.*$/'",
    "textile:upgrade": "npx ncu -u '/^@textile/.*$/'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@textile/hub": "^0.1.4",
    "@textile/textile": "^0.3.1",
    "@textile/threads-client": "^0.6.7",
    "@textile/threads-core": "^0.1.18",
    "@textile/threads-database": "^0.4.4",
    "@types/interface-datastore": "^0.8.0",
    "@types/js-cookie": "^2.2.6",
    "@types/uuid": "^7.0.3",
    "@vue/composition-api": "^0.6.3",
    "buffer": "^5.6.0",
    "core-js": "^3.6.4",
    "datastore-level": "^1.1.0",
    "direct-vuex": "^0.11.0",
    "interface-datastore": "^1.0.4",
    "js-cookie": "^2.2.1",
    "localforage": "^1.7.3",
    "register-service-worker": "^1.7.1",
    "rollup": "^1.32.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-e2e-cypress": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-pwa": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-typescript": "~4.3.0",
    "@vue/cli-plugin-unit-jest": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.0-beta.31",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.0.1",
    "node-sass": "^7.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "typescript": "~3.8.3",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "jest": {
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  }
}
